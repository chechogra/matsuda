{
  "env": {
    "browser": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "globals": {
    "jQuery":false,
    "$":true,
    "Modernizr": true,
    "Backbone": true,
    "_": true
  },
  "rules": {
    "comma-dangle": [2,"always"],
    "no-cond-assign": [2,"always"],
    "no-console": 2,
    "no-constant-condition": 2,
    "no-control-regex": 2,
    "no-debugger": 1,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-empty-character-class": 2,
    "no-empty": 2,
    "no-ex-assign": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-parens": [2,"all"],
    "no-extra-semi": 2,
    "no-func-assign": 2,
    "no-inner-declarations": [2,"both"],
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": 2,
    "no-negated-in-lhs": 2,
    "no-obj-calls": 2,
    "no-regex-spaces": 2,
    "no-sparse-arrays": 2,
    "no-unreachable": 2,
    "use-isnan": 2,
    "valid-typeof": 2,
    "no-unexpected-multiline": 2,
    "accessor-pairs": 2,
    "block-scoped-var": 2,
    "curly": [2,"all"],
    "default-case": 2,
    "eqeqeq": [2,"smart"],
    "no-alert": 2,
    "no-caller": 2,
    "no-eval": 2,
    "no-fallthrough": 2,
    "no-floating-decimal": 2,
    "no-invalid-this": 2,
    "no-loop-func": 2,
    "no-multi-str": 2,
    "no-new-func": 2,
    "no-new": 2,
    "no-octal-escape": 2,
    "no-octal": 2,
    "no-param-reassign": 2,
    "no-redeclare": 2,
    "no-return-assign": [2,"always"],
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-with": 2,
    "vars-on-top": 2,
    "yoda": [2,"always",{"exceptRange":true}],
    "no-delete-var": 2,
    "no-shadow-restricted-names": 2,
    "no-shadow": [2,{"hoist":"all"}],
    "no-undef-init": 2,
    "no-undef": 2,
    "no-undefined": 2,
    "no-unused-vars": 2,
    "no-use-before-define": 2,
    "brace-style": [2,"1tbs",{}],
    "camelcase": [2,{"properties":"always"}],
    "comma-spacing": [2,{"after":true}],
    "indent": [2, 2],
    "new-parens": 2,
    "no-lonely-if": 2,
    "no-multiple-empty-lines": [2,{"max":3}],
    "no-trailing-spaces": [2,{"skipBlankLines":true}],
    "space-infix-ops": 2
  }
}
